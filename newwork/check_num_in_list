liste_num = [34, 56, 89, 12, 44, 23, 40, 100, 27, 87, 83, 5, 16, 29, 66, 54, 69, 70, 83, 81, 33, 91, 3, 88, 14]

user_choice = int(input("Pick a number and see if its there:  "))

found = False

for liste in range(len(liste_num)):
  if liste_num[liste] == user_choice:
    print("Found")
    found = True 
    break
if not found:
  print("Not in ")
  
  
